{
   "Serilog": {
      "MinimumLevel": "Warning",
      "Override": {
         "Microsoft.AspNetCore": "Warning"
      },
      "WriteTo": [
         {
            "Name": "File",
            "Args": {
               "path": "serilog\\log.txt",
               "rollingInterval": "Day",
               "shared": true
            }
         }
      ]
   },
   "SpaDevSettings": {
      "UseSpaProxy": false,
      "ProxyUrl": "http://localhost:3010"
   },
   "AllowedHosts": "*"
}
